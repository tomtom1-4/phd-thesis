Cell[CellGroupData[{

Cell["Phase-Space Integral", "Section",
 CellChangeTimes->{{3.948367855915154*^9, 
  3.948367863499158*^9}},ExpressionUUID->"42000a60-8fc9-4fae-996e-\
ae70cd05e495"],

Cell[TextData[{
 "We evaluate the Phase Space Integral ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"P", ".", "S", "."}], "=", 
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["d", "d"], 
         SubscriptBox["p", "H"]}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"2", " ", "\[Pi]"}], ")"}], "d"]], " ", 
       FractionBox[
        RowBox[{
         SuperscriptBox["d", "d"], 
         SubscriptBox["p", "4"]}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"2", " ", "\[Pi]"}], ")"}], "d"]], " ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"2", " ", "\[Pi]"}], ")"}], "d"], " ", 
       RowBox[{
        SuperscriptBox["\[Delta]", 
         RowBox[{"(", "d", ")"}]], "(", 
        RowBox[{
         SubscriptBox["p", "H"], " ", "+", " ", 
         SubscriptBox["p", "4"], " ", "-", " ", "P"}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{"2", " ", "\[Pi]"}], ")"}], " ", 
       RowBox[{
        SubscriptBox["\[Delta]", "+"], "(", 
        RowBox[{
         SuperscriptBox[
          SubscriptBox["p", "H"], "2"], " ", "-", " ", 
         SuperscriptBox[
          SubscriptBox["m", "H"], "2"]}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{"2", " ", "\[Pi]"}], ")"}], " ", 
       RowBox[{
        SubscriptBox["\[Delta]", "+"], "(", 
        SuperscriptBox[
         SubscriptBox["p", "4"], "2"], ")"}]}]}], " ", "|", 
     RowBox[{"M", "(", 
      RowBox[{
       SubscriptBox["p", "H"], ".", "P"}], ")"}], 
     SuperscriptBox["|", "2"], " "}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "60ab58dc-10a4-4698-aff2-0df9126cf3a1"]
}], "Text",
 CellChangeTimes->{{3.948367889035548*^9, 3.948368141264385*^9}, {
  3.94836832246809*^9, 
  3.9483683246854057`*^9}},ExpressionUUID->"8b6f2533-81f6-4b9b-ab82-\
3f859537bbd9"],

Cell[TextData[{
 "We perform the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "4"], "  ", "integration", " ", "using", " ", "the", 
    " ", "delta", " ", "function", " "}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "53cee44a-cd62-4074-8e81-a107a6353ad0"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Delta]", 
     RowBox[{"(", "d", ")"}]], "(", 
    RowBox[{
     SubscriptBox["p", "H"], " ", "+", " ", 
     SubscriptBox["p", "4"], " ", "-", " ", "P"}], ")"}], TraditionalForm]],
  ExpressionUUID->"58ca1ee0-1e48-429b-817b-088ff5b6f9d0"],
 ". Subsequently the we perform the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["p", "H"], "0"], " ", "integral", " ", "using", " ", "the", 
    " "}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "eb8799e1-d6a5-4f80-9dea-bd31dfcbe34b"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Delta]", "+"], "(", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["p", "H"], "2"], " ", "-", " ", 
     SuperscriptBox[
      SubscriptBox["m", "H"], "2"]}], ")"}], TraditionalForm]],ExpressionUUID->
  "5b237e62-4538-41e9-a9ba-f90c5333c3b1"],
 " delta function\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"P", ".", "S", "."}], "=", 
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["d", 
         RowBox[{"d", "-", "1"}]], 
        SubscriptBox["p", "H"]}], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"2", " ", "\[Pi]"}], ")"}], 
         RowBox[{"d", "-", "1"}]], " ", 
        SqrtBox[
         RowBox[{
          RowBox[{
           SuperscriptBox[
            SubscriptBox["m", "H"], "2"], " ", "+"}], " ", "|", 
          SubscriptBox["p", "H"], 
          SuperscriptBox["|", "2"]}]]}]], "   ", 
      RowBox[{"(", 
       RowBox[{"2", " ", "\[Pi]"}], ")"}], " ", 
      SubscriptBox["\[Delta]", "+"], 
      RowBox[{"(", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"P", " ", "-", " ", 
          SubscriptBox["p", "H"]}], ")"}], "2"], ")"}]}]}], " ", "|", 
    RowBox[{"M", "(", 
     RowBox[{
      SubscriptBox["p", "H"], ".", "P"}], ")"}], 
    SuperscriptBox["|", "2"]}]}]],ExpressionUUID->
  "959ae3b6-244e-4a1d-a7cc-2ed3681a84ea"],
 " "
}], "Text",
 CellChangeTimes->{{3.948368249705018*^9, 3.948368431591062*^9}, {
  3.948368893382559*^9, 
  3.9483688935672407`*^9}},ExpressionUUID->"3a98896a-2e85-4d25-ab0b-\
ac0e202b8ea7"],

Cell[TextData[{
 "In the center of mass frame (COM) the initial state momentum is given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["P", "0"], ",", "0", ",", "0", ",", "0"}], ")"}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4bc751be-981f-4d60-b36f-13fe69a3c7ef"],
 " => ",
 Cell[BoxData[
  SuperscriptBox[
   RowBox[{"(", 
    RowBox[{"P", " ", "-", " ", 
     SubscriptBox["p", "H"]}], ")"}], "2"]],ExpressionUUID->
  "830fe4ac-0a58-43a5-9a75-e20cca09342f"],
 "= ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["P", "0"], "2"], " ", "-", " ", 
    RowBox[{"2", " ", 
     SubscriptBox["P", "0"], 
     SqrtBox[
      RowBox[{
       RowBox[{
        SuperscriptBox[
         SubscriptBox["m", "H"], "2"], " ", "+"}], " ", "|", 
       SubscriptBox["p", "H"], 
       SuperscriptBox["|", "2"]}]]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3c06d6eb-63ea-46fa-a211-c27561dbf203"],
 ". We use spherical coordinates ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"\[Integral]", 
    RowBox[{
     SuperscriptBox["d", 
      RowBox[{"d", "-", "1"}]], 
     SubscriptBox["p", "H"]}]}], " ", "=", " ", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Integral]", 
      SuperscriptBox[
       SubscriptBox["S", "1"], 
       RowBox[{"d", " ", "-", " ", "2"}]]], 
     RowBox[{"d\[CapitalOmega]", 
      RowBox[{"\[Integral]", " ", 
       SubscriptBox["dp", "H"]}]}]}], "|", 
    SubscriptBox["p", "H"], 
    SuperscriptBox["|", 
     RowBox[{"d", " ", "-", " ", "2"}]], "  "}]}]],ExpressionUUID->
  "39dfff3e-23d2-49e3-b45a-fbef2ea106c0"],
 "and solve the ",
 Cell[BoxData[
  SubscriptBox["p", "H"]],ExpressionUUID->
  "107ff664-5bf4-4d5d-a2cc-fc38b526c388"],
 " integral using the last delta function"
}], "Text",
 CellChangeTimes->{{3.948368450740438*^9, 3.9483685470486593`*^9}, {
  3.9483685823434153`*^9, 
  3.94836881972966*^9}},ExpressionUUID->"1b638f79-b5b8-45e1-8049-\
716b9963fc22"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Assuming", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"P0", ">", "0"}], "&&", " ", 
    RowBox[{"mH", ">", " ", "0"}], "&&", " ", 
    RowBox[{"p3", " ", ">", "0"}], "&&", " ", 
    RowBox[{"mH", " ", "<", " ", "P0"}]}], ",", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"DiracDelta", "[", 
       RowBox[{
        SuperscriptBox["P0", "2"], " ", "-", " ", 
        RowBox[{"2", " ", "P0", " ", 
         RowBox[{"(", 
          SqrtBox[
           RowBox[{
            SuperscriptBox["p3", "2"], " ", "+", " ", 
            SuperscriptBox["mH", "2"]}]], " ", ")"}]}], "+", " ", 
        SuperscriptBox["mH", "2"]}], "]"}], " ", 
      FractionBox[
       SuperscriptBox["p3", 
        RowBox[{"d", " ", "-", " ", "2"}]], 
       SqrtBox[
        RowBox[{
         SuperscriptBox["p3", "2"], " ", "+", " ", 
         SuperscriptBox["mH", "2"]}]]]}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"p3", ",", " ", "0", ",", " ", "Infinity"}], "}"}]}], "]"}]}], 
  "]"}]], "Input",
 CellChangeTimes->{3.94836887254533*^9},
 CellLabel->"In[59]:=",ExpressionUUID->"5e1909a6-012b-4ce0-82e2-54871872c858"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["2", 
   RowBox[{"2", "-", "d"}]], " ", 
  SuperscriptBox["P0", 
   RowBox[{"2", "-", "d"}]], " ", 
  SuperscriptBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["mH", "2"]}], "+", 
     SuperscriptBox["P0", "2"]}], ")"}], 
   RowBox[{
    RowBox[{"-", "3"}], "+", "d"}]]}]], "Output",
 CellChangeTimes->{3.9483688454373283`*^9, 3.948368886110093*^9},
 CellLabel->"Out[59]=",ExpressionUUID->"7942e16d-7802-481f-becc-d91adbab0078"]
}, Open  ]],

Cell[TextData[{
 "The Result is \n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"P", ".", "S", "."}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"2", " ", "\[Pi]"}], ")"}], 
       RowBox[{"d", "-", "2"}]]], 
     RowBox[{
      SubscriptBox["\[Integral]", 
       SuperscriptBox[
        SubscriptBox["S", "1"], 
        RowBox[{"d", " ", "-", " ", "2"}]]], 
      RowBox[{"d\[CapitalOmega]", " ", 
       SuperscriptBox["2", 
        RowBox[{"2", "-", "d"}]], " ", 
       SuperscriptBox[
        SubscriptBox["P", "0"], 
        RowBox[{"2", "-", "d"}]], " ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox[
            SubscriptBox["m", "H"], "2"]}], "+", 
          SuperscriptBox[
           SubscriptBox["P", "0"], "2"]}], ")"}], 
        RowBox[{
         RowBox[{"-", "3"}], "+", "d"}]]}]}]}], " ", "|", 
    RowBox[{"M", "(", 
     RowBox[{
      SubscriptBox["p", "H"], ".", "P"}], ")"}], 
    SuperscriptBox["|", "2"]}]}]],ExpressionUUID->
  "854cb022-95f9-4092-a45d-94b2cbbc642b"],
 " \nWe now use the recursion relation (see ",
 ButtonBox["Czakon and Heymes, 2014",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://arxiv.org/abs/1408.2500"], None},
  ButtonNote->"https://arxiv.org/abs/1408.2500"],
 "  Eq. (B.5))\n",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["\[Integral]", 
    SuperscriptBox[
     SubscriptBox["S", "1"], 
     RowBox[{"d", " ", "-", " ", "2"}]]], "d\[CapitalOmega]"}]],
  ExpressionUUID->"c3fff657-0c36-440c-a462-4395372a7f2e"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Integral]", 
    RowBox[{"dcos", " ", "\[Theta]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f8b84f80-ddd5-4fa5-9d46-8de0e901f559"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["sin", 
     RowBox[{"d", " ", "-", " ", "4"}]], "\[Theta]", " "}], TraditionalForm]],
  
  FormatType->TraditionalForm,ExpressionUUID->
  "4cde7437-c53d-45fb-b4e1-5bdea44913b2"],
 Cell[BoxData[
  RowBox[{
   SubscriptBox["\[Integral]", 
    SuperscriptBox[
     SubscriptBox["S", "1"], 
     RowBox[{"d", " ", "-", " ", "3"}]]], "d\[CapitalOmega]"}]],
  ExpressionUUID->"4a275b68-b1a0-4dee-b9c7-36701af5f6e9"],
 " \nSince the matrix Element only depends on the azimuthal angle, we can \
carry out the remaining integration\n",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["\[Integral]", 
    SuperscriptBox[
     SubscriptBox["S", "1"], 
     RowBox[{"d", " ", "-", " ", "3"}]]], "d\[CapitalOmega]"}]],
  ExpressionUUID->"b26f0f0c-e216-43f8-a71c-bbd105d8291d"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"2", " ", 
      SuperscriptBox["\[Pi]", 
       FractionBox[
        RowBox[{"d", " ", "-", " ", "2"}], "2"]]}], 
     RowBox[{"\[CapitalGamma]", "(", 
      FractionBox[
       RowBox[{"d", " ", "-", " ", "2"}], "2"], ")"}]], " "}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9242c531-8f89-4bb9-80c0-aca2c8c6f56a"],
 "\nHence the resulting Phase-Space integral reads \n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"P", ".", "S", "."}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}], 
       RowBox[{"d", "-", "2"}]]], " ", 
     FractionBox[
      RowBox[{"2", " ", 
       SuperscriptBox["\[Pi]", 
        FractionBox[
         RowBox[{"d", " ", "-", " ", "2"}], "2"]]}], 
      RowBox[{"\[CapitalGamma]", "(", 
       FractionBox[
        RowBox[{"d", " ", "-", " ", "2"}], "2"], ")"}]], "  ", 
     SuperscriptBox["2", 
      RowBox[{"2", "-", "d"}]], " ", 
     SuperscriptBox[
      SubscriptBox["P", "0"], 
      RowBox[{"2", "-", "d"}]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", 
         SuperscriptBox[
          SubscriptBox["m", "H"], "2"]}], "+", 
        SuperscriptBox[
         SubscriptBox["P", "0"], "2"]}], ")"}], 
      RowBox[{
       RowBox[{"-", "3"}], "+", "d"}]], " ", 
     FormBox[
      RowBox[{"\[Integral]", 
       RowBox[{"dcos", " ", "\[Theta]"}]}],
      TraditionalForm], "  ", 
     SuperscriptBox["2", 
      RowBox[{"d", " ", "-", " ", "4"}]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"1", " ", "-", " ", "cos\[Theta]"}], "2"], ")"}], 
      FractionBox[
       RowBox[{"d", "-", "4"}], "2"]], 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"1", " ", "+", " ", "cos\[Theta]"}], "2"], ")"}], 
      FractionBox[
       RowBox[{"d", "-", "4"}], "2"]]}], "|", 
    RowBox[{"M", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["p", "H"], ".", "P"}], ")"}]}], 
    SuperscriptBox["|", "2"]}]}]],ExpressionUUID->
  "ec862c02-1d55-4dc0-92b8-3d746dfeb785"],
 "  \nFinally, we substitute \n\[Omega] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", " ", 
    FractionBox[
