Notebook[{

Cell[CellGroupData[{
Cell["Heavy Top Limit Cross Section calculation at NLO", "Title",
 CellChangeTimes->{{3.948367692577159*^9, 
  3.9483677083057756`*^9}},ExpressionUUID->"f2319075-7550-4a23-a4a2-\
c8d6a0e0c326"],

Cell[TextData[{
 "The analysis closely follows ",
 ButtonBox["Dawson, 1991",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://www.sciencedirect.com/science/article/pii/0550321391900612?\
via%3Dihub"], None},
  ButtonNote->
   "https://www.sciencedirect.com/science/article/pii/0550321391900612?via%\
3Dihub"]
}], "Text",
 CellChangeTimes->{{3.948367718369396*^9, 
  3.948367754063273*^9}},ExpressionUUID->"a47302cc-331e-420f-a12a-\
1173ddf3b0a0"],

Cell[CellGroupData[{

Cell["Phase-Space Integral", "Section",
 CellChangeTimes->{{3.948367855915154*^9, 
  3.948367863499158*^9}},ExpressionUUID->"38030550-0400-44d9-9130-\
67d8c158661f"],

Cell[TextData[{
 "We evaluate the Phase Space Integral ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"P", ".", "S", "."}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox[Cell[
       "\[Mu]",ExpressionUUID->"3530f3a7-bf25-4293-8401-be98a6313671"], 
       RowBox[{"2", "\[Epsilon]"}]], 
      RowBox[{"\[Integral]", 
       RowBox[{
        FractionBox[
         RowBox[{
          SuperscriptBox["d", "d"], 
          SubscriptBox["p", "H"]}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"2", " ", "\[Pi]"}], ")"}], "d"]], " ", 
        FractionBox[
         RowBox[{
          SuperscriptBox["d", "d"], 
          SubscriptBox["p", "4"]}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"2", " ", "\[Pi]"}], ")"}], "d"]], " ", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"2", " ", "\[Pi]"}], ")"}], "d"], " ", 
        RowBox[{
         SuperscriptBox["\[Delta]", 
          RowBox[{"(", "d", ")"}]], "(", 
         RowBox[{
          SubscriptBox["p", "H"], " ", "+", " ", 
          SubscriptBox["p", "4"], " ", "-", " ", "P"}], ")"}], " ", 
        RowBox[{"(", 
         RowBox[{"2", " ", "\[Pi]"}], ")"}], " ", 
        RowBox[{
         SubscriptBox["\[Delta]", "+"], "(", 
         RowBox[{
          SuperscriptBox[
           SubscriptBox["p", "H"], "2"], " ", "-", " ", 
          SuperscriptBox[
           SubscriptBox["m", "H"], "2"]}], ")"}], " ", 
        RowBox[{"(", 
         RowBox[{"2", " ", "\[Pi]"}], ")"}], " ", 
        RowBox[{
         SubscriptBox["\[Delta]", "+"], "(", 
         SuperscriptBox[
          SubscriptBox["p", "4"], "2"], ")"}]}]}]}], " ", "|", 
     RowBox[{"M", "(", 
      RowBox[{
       SubscriptBox["p", "H"], ".", "P"}], ")"}], 
     SuperscriptBox["|", "2"], " "}]}], TraditionalForm]],ExpressionUUID->
  "0c18b18c-9fec-4a92-8006-956de9c38173"]
}], "Text",
 CellChangeTimes->{{3.948367889035548*^9, 3.948368141264385*^9}, {
  3.94836832246809*^9, 3.9483683246854057`*^9}, {3.948446393174767*^9, 
  3.948446470359128*^9}, {3.9484465540592213`*^9, 
  3.948446555143422*^9}},ExpressionUUID->"47d827b8-2702-4115-92ad-\
1a8922b64f14"],

Cell[TextData[{
 "We perform the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["p", "4"], "  ", "integration", " ", "using", " ", "the", 
    " ", "delta", " ", "function", " "}], TraditionalForm]],ExpressionUUID->
  "d93e1f61-1fd4-43f2-87df-3f762977a40c"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Delta]", 
     RowBox[{"(", "d", ")"}]], "(", 
    RowBox[{
     SubscriptBox["p", "H"], " ", "+", " ", 
     SubscriptBox["p", "4"], " ", "-", " ", "P"}], ")"}], TraditionalForm]],
  ExpressionUUID->"ea4296b5-ff0d-4a04-a041-b9c8f2bf6aea"],
 ". Subsequently the we perform the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["p", "H"], "0"], " ", "integral", " ", "using", " ", "the", 
    " "}], TraditionalForm]],ExpressionUUID->
  "99f82474-6625-4ccd-8843-f143ed84aa8f"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Delta]", "+"], "(", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["p", "H"], "2"], " ", "-", " ", 
     SuperscriptBox[
      SubscriptBox["m", "H"], "2"]}], ")"}], TraditionalForm]],ExpressionUUID->
  "9f060f6b-e65c-4f81-a14a-d6a107e6fbde"],
 " delta function\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"P", ".", "S", "."}], "=", 
   RowBox[{
    RowBox[{"\[Integral]", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["d", 
         RowBox[{"d", "-", "1"}]], 
        SubscriptBox["p", "H"]}], 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"2", " ", "\[Pi]"}], ")"}], 
         RowBox[{"d", "-", "1"}]], " ", 
        SqrtBox[
         RowBox[{
          RowBox[{
           SuperscriptBox[
            SubscriptBox["m", "H"], "2"], " ", "+"}], " ", "|", 
          SubscriptBox["p", "H"], 
          SuperscriptBox["|", "2"]}]]}]], "   ", 
      RowBox[{"(", 
       RowBox[{"2", " ", "\[Pi]"}], ")"}], " ", 
      SubscriptBox["\[Delta]", "+"], 
      RowBox[{"(", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"P", " ", "-", " ", 
          SubscriptBox["p", "H"]}], ")"}], "2"], ")"}]}]}], " ", "|", 
    RowBox[{"M", "(", 
     RowBox[{
      SubscriptBox["p", "H"], ".", "P"}], ")"}], 
    SuperscriptBox["|", "2"]}]}]],ExpressionUUID->
  "d05e4576-586b-49c1-9698-c96d05059524"],
 " "
}], "Text",
 CellChangeTimes->{{3.948368249705018*^9, 3.948368431591062*^9}, {
  3.948368893382559*^9, 
  3.9483688935672407`*^9}},ExpressionUUID->"2fd7b56e-a0f3-4121-acc3-\
57bd51910a74"],

Cell[TextData[{
 "In the center of mass frame (COM) the initial state momentum is given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["P", "0"], ",", "0", ",", "0", ",", "0"}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"1031d4a9-fa17-4316-b4ca-9393dfdea3c6"],
 " => ",
 Cell[BoxData[
  SuperscriptBox[
   RowBox[{"(", 
    RowBox[{"P", " ", "-", " ", 
     SubscriptBox["p", "H"]}], ")"}], "2"]],ExpressionUUID->
  "c0617811-5a4c-4cbe-894e-de3ab724c2c3"],
 "= ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["P", "0"], "2"], " ", "-", " ", 
    RowBox[{"2", " ", 
     SubscriptBox["P", "0"], 
     SqrtBox[
      RowBox[{
       RowBox[{
        SuperscriptBox[
         SubscriptBox["m", "H"], "2"], " ", "+"}], " ", "|", 
       SubscriptBox["p", "H"], 
       SuperscriptBox["|", "2"]}]]}]}], TraditionalForm]],ExpressionUUID->
  "ead33e02-68d3-4665-868f-ba3a9cd10d03"],
 ". We use spherical coordinates ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"\[Integral]", 
    RowBox[{
     SuperscriptBox["d", 
      RowBox[{"d", "-", "1"}]], 
     SubscriptBox["p", "H"]}]}], " ", "=", " ", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Integral]", 
      SuperscriptBox[
       SubscriptBox["S", "1"], 
       RowBox[{"d", " ", "-", " ", "2"}]]], 
     RowBox[{"d\[CapitalOmega]", 
      RowBox[{"\[Integral]", " ", 
       SubscriptBox["dp", "H"]}]}]}], "|", 
    SubscriptBox["p", "H"], 
    SuperscriptBox["|", 
     RowBox[{"d", " ", "-", " ", "2"}]], "  "}]}]],ExpressionUUID->
  "f59e58cc-1186-44fa-8896-8ee0fbda7737"],
 "and solve the ",
 Cell[BoxData[
  SubscriptBox["p", "H"]],ExpressionUUID->
  "4db73d78-0e7d-42ec-83e8-0a12ee727e59"],
 " integral using the last delta function"
}], "Text",
 CellChangeTimes->{{3.948368450740438*^9, 3.9483685470486593`*^9}, {
  3.9483685823434153`*^9, 
  3.94836881972966*^9}},ExpressionUUID->"da4421d7-ca4a-4371-8572-\
94e24e131de0"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Assuming", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"P0", ">", "0"}], "&&", " ", 
    RowBox[{"mH", ">", " ", "0"}], "&&", " ", 
    RowBox[{"p3", " ", ">", "0"}], "&&", " ", 
    RowBox[{"mH", " ", "<", " ", "P0"}]}], ",", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"DiracDelta", "[", 
       RowBox[{
        SuperscriptBox["P0", "2"], " ", "-", " ", 
        RowBox[{"2", " ", "P0", " ", 
         RowBox[{"(", 
          SqrtBox[
           RowBox[{
            SuperscriptBox["p3", "2"], " ", "+", " ", 
            SuperscriptBox["mH", "2"]}]], " ", ")"}]}], "+", " ", 
        SuperscriptBox["mH", "2"]}], "]"}], " ", 
      FractionBox[
       SuperscriptBox["p3", 
        RowBox[{"d", " ", "-", " ", "2"}]], 
       SqrtBox[
        RowBox[{
         SuperscriptBox["p3", "2"], " ", "+", " ", 
         SuperscriptBox["mH", "2"]}]]]}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"p3", ",", " ", "0", ",", " ", "Infinity"}], "}"}]}], "]"}]}], 
  "]"}]], "Input",
 CellChangeTimes->{3.94836887254533*^9},
 CellLabel->"In[54]:=",ExpressionUUID->"017738e3-9706-43ba-a250-f4bf7427f1d3"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["2", 
   RowBox[{"2", "-", "d"}]], " ", 
  SuperscriptBox["P0", 
   RowBox[{"2", "-", "d"}]], " ", 
  SuperscriptBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["mH", "2"]}], "+", 
     SuperscriptBox["P0", "2"]}], ")"}], 
   RowBox[{
    RowBox[{"-", "3"}], "+", "d"}]]}]], "Output",
 CellChangeTimes->{3.9483688454373283`*^9, 3.948368886110093*^9, 
  3.948775339564125*^9, 3.94888242232366*^9},
 CellLabel->"Out[54]=",ExpressionUUID->"1dfe997f-6b81-40f0-88db-803d94ba1ad2"]
}, Open  ]],

Cell[TextData[{
 "The Result is \n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"P", ".", "S", "."}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"2", " ", "\[Pi]"}], ")"}], 
       RowBox[{"d", "-", "2"}]]], 
     RowBox[{
      SubscriptBox["\[Integral]", 
       SuperscriptBox[
        SubscriptBox["S", "1"], 
        RowBox[{"d", " ", "-", " ", "2"}]]], 
      RowBox[{"d\[CapitalOmega]", " ", 
       SuperscriptBox["2", 
        RowBox[{"2", "-", "d"}]], " ", 
       SuperscriptBox[
        SubscriptBox["P", "0"], 
        RowBox[{"2", "-", "d"}]], " ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox[
            SubscriptBox["m", "H"], "2"]}], "+", 
          SuperscriptBox[
           SubscriptBox["P", "0"], "2"]}], ")"}], 
        RowBox[{
         RowBox[{"-", "3"}], "+", "d"}]]}]}]}], " ", "|", 
    RowBox[{"M", "(", 
     RowBox[{
      SubscriptBox["p", "H"], ".", "P"}], ")"}], 
    SuperscriptBox["|", "2"]}]}]],ExpressionUUID->
  "d7db5e18-1c6e-40bd-b871-efec91854dc6"],
 " \nWe now use the recursion relation (see ",
 ButtonBox["Czakon and Heymes, 2014",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://arxiv.org/abs/1408.2500"], None},
  ButtonNote->"https://arxiv.org/abs/1408.2500"],
 "  Eq. (B.5))\n",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["\[Integral]", 
    SuperscriptBox[
     SubscriptBox["S", "1"], 
     RowBox[{"d", " ", "-", " ", "2"}]]], "d\[CapitalOmega]"}]],
  ExpressionUUID->"de2d8b44-aa94-4cba-ad75-f551fa0e5bbe"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Integral]", 
    RowBox[{"dcos", " ", "\[Theta]"}]}], TraditionalForm]],ExpressionUUID->
  "5a5d4496-ed2a-4045-b862-116e28011f53"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["sin", 
     RowBox[{"d", " ", "-", " ", "4"}]], "\[Theta]", " "}], TraditionalForm]],
  ExpressionUUID->"bdf673ce-f617-452b-a8b0-1f7f80911d91"],
 Cell[BoxData[
  RowBox[{
   SubscriptBox["\[Integral]", 
    SuperscriptBox[
     SubscriptBox["S", "1"], 
     RowBox[{"d", " ", "-", " ", "3"}]]], "d\[CapitalOmega]"}]],
  ExpressionUUID->"84fce323-b9c8-4c1c-8635-07532852d46f"],
 " \nSince the matrix Element only depends on the azimuthal angle, we can \
carry out the remaining integration\n",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["\[Integral]", 
    SuperscriptBox[
     SubscriptBox["S", "1"], 
     RowBox[{"d", " ", "-", " ", "3"}]]], "d\[CapitalOmega]"}]],
  ExpressionUUID->"27daaf6b-cdf9-43dd-bda9-45ac169a97fe"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"2", " ", 
      SuperscriptBox["\[Pi]", 
       FractionBox[
        RowBox[{"d", " ", "-", " ", "2"}], "2"]]}], 
     RowBox[{"\[CapitalGamma]", "(", 
      FractionBox[
       RowBox[{"d", " ", "-", " ", "2"}], "2"], ")"}]], " "}], 
   TraditionalForm]],ExpressionUUID->"637ddaaa-3a54-4ec7-ac76-990c4a756527"],
 "\nHence the resulting Phase-Space integral reads \n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"P", ".", "S", "."}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}], 
       RowBox[{"d", "-", "2"}]]], " ", 
     FractionBox[
      RowBox[{"2", " ", 
       SuperscriptBox["\[Pi]", 
        FractionBox[
         RowBox[{"d", " ", "-", " ", "2"}], "2"]]}], 
      RowBox[{"\[CapitalGamma]", "(", 
       FractionBox[
        RowBox[{"d", " ", "-", " ", "2"}], "2"], ")"}]], "  ", 
     SuperscriptBox["2", 
      RowBox[{"2", "-", "d"}]], " ", 
     SuperscriptBox[
      SubscriptBox["P", "0"], 
      RowBox[{"2", "-", "d"}]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", 
         SuperscriptBox[
          SubscriptBox["m", "H"], "2"]}], "+", 
        SuperscriptBox[
         SubscriptBox["P", "0"], "2"]}], ")"}], 
      RowBox[{
       RowBox[{"-", "3"}], "+", "d"}]], " ", 
     FormBox[
      RowBox[{"\[Integral]", 
       RowBox[{"dcos", " ", "\[Theta]"}]}],
      TraditionalForm], "  ", 
     SuperscriptBox["2", 
      RowBox[{"d", " ", "-", " ", "4"}]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"1", " ", "-", " ", "cos\[Theta]"}], "2"], ")"}], 
      FractionBox[
       RowBox[{"d", "-", "4"}], "2"]], 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"1", " ", "+", " ", "cos\[Theta]"}], "2"], ")"}], 
      FractionBox[
       RowBox[{"d", "-", "4"}], "2"]]}], "|", 
    RowBox[{"M", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["p", "H"], ".", "P"}], ")"}]}], 
    SuperscriptBox["|", "2"]}]}]],ExpressionUUID->
  "7f611e70-0768-4721-b679-adf41e33edb3"],
 "  \nFinally, we substitute \n\[Omega] ",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", " ", 
    FractionBox[
     RowBox[{"1", "+", "cos\[Theta]"}], "2"]}], TraditionalForm]],
  ExpressionUUID->"baa63c01-f07d-4bbc-b7eb-ee621f5cda88"],
 ", 1 - \[Omega] = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"1", "-", "cos\[Theta]"}], "2"], ",", " ", 
    RowBox[{"cos\[Theta]", " ", "=", " ", 
     RowBox[{
      RowBox[{"2", " ", "\[Omega]"}], " ", "-", " ", "1"}]}]}], 
   TraditionalForm]],ExpressionUUID->"afe170f9-205c-46a9-83df-bcb625172dcb"],
 " \n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"P", ".", "S", "."}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}], 
       RowBox[{"d", "-", "2"}]]], " ", 
     FractionBox[
      RowBox[{"2", " ", 
       SuperscriptBox["\[Pi]", 
        FractionBox[
         RowBox[{"d", " ", "-", " ", "2"}], "2"]]}], 
      RowBox[{"\[CapitalGamma]", "(", 
       FractionBox[
        RowBox[{"d", " ", "-", " ", "2"}], "2"], ")"}]], "  ", 
     FractionBox["1", "4"], " ", 
     SuperscriptBox[
      SubscriptBox["P", "0"], 
      RowBox[{"2", "-", "d"}]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", 
         SuperscriptBox[
          SubscriptBox["m", "H"], "2"]}], "+", 
        SuperscriptBox[
         SubscriptBox["P", "0"], "2"]}], ")"}], 
      RowBox[{
       RowBox[{"-", "3"}], "+", "d"}]], " ", 
     FormBox[
      RowBox[{
       SuperscriptBox[
        SubscriptBox["\[Integral]", "0"], "1"], "d\[Omega]"}],
      TraditionalForm], "   ", 
     SuperscriptBox["\[Omega]", 
      RowBox[{"-", "\[Epsilon]"}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", " ", "-", " ", "\[Omega]"}], ")"}], 
      RowBox[{"-", "\[Epsilon]"}]]}], "|", 
    RowBox[{"M", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["p", "H"], ".", "P"}], ")"}]}], 
    SuperscriptBox["|", "2"]}]}]],ExpressionUUID->
  "74ff50f9-b5ac-43e5-ae66-377d17a59d7e"]
}], "Text",
 CellChangeTimes->{{3.948368921980426*^9, 3.9483690221678257`*^9}, {
  3.948369065078498*^9, 3.94836918712071*^9}, {3.948369225225046*^9, 
  3.9483694696394377`*^9}, {3.948369503518211*^9, 3.9483695873945637`*^9}, {
  3.9483696675760107`*^9, 3.94836975655846*^9}, {3.948369867300008*^9, 
  3.948369967993548*^9}},ExpressionUUID->"dfa6865a-1a31-4325-86cf-\
724b1ed457e6"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Assuming", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"s", ">", "0"}], "&&", " ", 
    RowBox[{"mH", ">", " ", "0"}], "&&", " ", 
    RowBox[{"mH", " ", "<", " ", "s"}]}], ",", 
   RowBox[{"FullSimplify", "[", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"2", " ", "Pi"}], ")"}], 
        RowBox[{"d", " ", "-", " ", "2"}]]], " ", "2", " ", 
      FractionBox[
       SuperscriptBox["Pi", 
        FractionBox[
         RowBox[{"d", " ", "-", " ", "2"}], "2"]], 
       RowBox[{"Gamma", "[", 
        FractionBox[
         RowBox[{"d", " ", "-", " ", "2"}], "2"], "]"}]], " ", 
      FractionBox["1", "4"], " ", 
      SuperscriptBox["s", 
       FractionBox[
        RowBox[{"2", " ", "-", " ", "d"}], "2"]], " ", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", 
          SuperscriptBox["mH", "2"]}], " ", "+", " ", "s"}], ")"}], 
       RowBox[{"d", " ", "-", " ", "3"}]]}], "/.", 
     RowBox[{"{", 
      RowBox[{"d", " ", "\[Rule]", " ", 
       RowBox[{"4", " ", "-", " ", 
        RowBox[{"2", " ", "\[Epsilon]"}]}]}], "}"}]}], "]"}]}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.948370329479673*^9, 3.9483704902810993`*^9}},
 CellLabel->"In[55]:=",ExpressionUUID->"97bbd4ff-272b-497d-808f-3f735ecc3d51"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   SuperscriptBox["2", 
    RowBox[{
     RowBox[{"-", "3"}], "+", 
     RowBox[{"2", " ", "\[Epsilon]"}]}]], " ", 
   SuperscriptBox["\[Pi]", 
    RowBox[{
     RowBox[{"-", "1"}], "+", "\[Epsilon]"}]], " ", 
   SuperscriptBox["s", 
    RowBox[{
     RowBox[{"-", "1"}], "+", "\[Epsilon]"}]], " ", 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox["mH", "2"]}], "+", "s"}], ")"}], 
    RowBox[{"1", "-", 
     RowBox[{"2", " ", "\[Epsilon]"}]}]]}], 
  RowBox[{"Gamma", "[", 
